mod Number
    traits
        Value: Int
        Add: (other: Self) -> Self

mod OnePlus
    class
        value: Int
    defs
        Number\Value: value + 1
        Number\Add
            OnePlus
                value: other.Value + Self.Value - 1

lets
    Main: Int
        OnePlus(value: 1).Add
            other: OnePlus(value: 2).Add
                other: OnePlus(value: 3)
        .Value
